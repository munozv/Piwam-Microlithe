div.autocomplete {
    position: absolute;
    background-color: white;
    border: 1px solid #C8C8C8;
    margin: 0px;
    padding: 0px;
}

div.autocomplete ul {
    list-style-type: none;
    margin: 0px;
    padding: 0px;
    max-height: 20em;
    overflow: auto;
}

div.autocomplete ul li.selected {
    background-color: #FCF5DD;
}

div.autocomplete ul li {
    list-style-type: none;
    display: block;
    margin: 0;
    padding: 5px;
    cursor: pointer;
}